student={
    "name" : "archit",
    "father" : "navin",
    "mother" : "archana",
    "age":{
        "papa" : 42,
        "maa"  : 38,
        "didi" : 24,
        "my"   : 19
    }
}
print(tuple(student["age"]))
print(list(student))
print(student.keys())
print(student["age"].keys())
print(student.items())
print(student["age"].get("papa"))
print(student.update({"city" : "kanpur"}))
print(type(student))
print(student)
